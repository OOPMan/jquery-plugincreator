  "readmeFilename": "README.md",
  "dependencies": {
    "jquery": ">=1.9.0",
    "esprima": ">=1.2.2"
  },
  "devDependencies": {
    "babel-cli": ">=6.9.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": ">=6.9.0",
    "mocha": ">=2.0.0",
    "unit.js": ">=1.0.2"
  }
