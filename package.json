{
  "name": "jquery-plugincreator",
  "version": "0.3.0",
  "description": "A jQuery add-on that makes creating plugins a little easier.",
  "main": "./es6/jquery.plugincreator.es6",
  "scripts": {
    "test": "babel-node ./node_modules/mocha/bin/mocha -C -R spec",
    "build": "babel es6 --presets es2015 --plugins transform-es2015-modules-umd --out-dir dist"
